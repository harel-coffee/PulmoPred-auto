<config-root>
    <n_folds>5</n_folds>
    <random_state>42</random_state>
    <n_US_folds>
        <pft>6</pft>
        <tct>1</tct>
    </n_US_folds>
    <estimator_ids>
        <svm>SVM</svm>
        <rf>RF</rf>
        <nb>GNB</nb>
    </estimator_ids>
    <optimal_hyperparameters>
        <pft>
            <svm>
                <kernel>rbf</kernel>
                <C>5</C>
                <gamma>0.0001</gamma>
                <thresholds>
                    <threshold>0.1</threshold>
                    <threshold>0.2</threshold>
                    <threshold>0.3</threshold>
                    <threshold>-0.1</threshold>
                    <threshold>0.2</threshold>
                    <threshold>0.1</threshold>
                </thresholds>
            </svm>
            <rf>
                <n_estimators>20</n_estimators>
                <max_depth>4</max_depth>
                <max_features>0.4</max_features>
            </rf>
            <nb>
                <smoothing>0</smoothing>
            </nb>
        </pft>
        <tct>
            <svm>
                <kernel>rbf</kernel>
                <C>10</C>
                <gamma>0.0001</gamma>
                <thresholds>
                    <threshold>0.3</threshold>
                </thresholds>
            </svm>
            <rf>
                <n_estimators>30</n_estimators>
                <max_depth>8</max_depth>
                <max_features>0.5</max_features>
            </rf>
            <nb>
                <smoothing>0.1</smoothing>
            </nb>
        </tct>
    </optimal_hyperparameters>
</config-root>
